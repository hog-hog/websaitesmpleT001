  <title>My Website</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      margin: 0;
      padding: 0;
    }

    header {
      background-color: #333;
      color: #fff;
      padding: 10px;
    }

    nav {
      background-color: #f4f4f4;
      width: 200px;
      padding: 10px;
      float: left;
    }

    main {
      padding: 10px;
      margin-left: 220px;
      margin-right: 220px;
    }

    aside {
      background-color: #f4f4f4;
      width: 200px;
      padding: 10px;
      float: right;
    }

    article {
      margin-top: 20px;
    }

    button {
      background-color: #333;
      color: #fff;
      padding: 10px;
      border: none;
      cursor: pointer;
      margin-bottom: 10px;
    }

    img {
      width: 100%;
      height: auto;
      margin-bottom: 10px;
    }

    footer {
      background-color: #333;
      color: #fff;
      padding: 10px;
      clear: both;
    }
  </style>
