<body>
  <header>
    <h1>Header</h1>
  </header>
  <nav>
    <p>SID</p>
  </nav>
  <main>
    <p>M</p>
    <section>
      <p>K</p>
      <button id="button1">B1</button>
      <button id="button2">B2</button>
      <div id="imageContainer">
        <img src="image1.jpg" alt="Image 1">
        <img src="image2.jpg" alt="Image 2">
        <img src="image3.jpg" alt="Image 3">
      </div>
    </section>
  </main>
  <aside>
    <img src="imageG1.jpg" alt="Image G1">
    <img src="imageG2.jpg" alt="Image G2">
    <img src="imageG3.jpg" alt="Image G3">
  </aside>
  <footer>
    <p>Footer</p>
  </footer>
  <script>
    var button1 = document.getElementById('button1');
    var button2 = document.getElementById('button2');
    var imageContainer = document.getElementById('imageContainer');
    var images = Array.from(imageContainer.getElementsByTagName('img'));

    button1.addEventListener('click', function() {
      var lastImage = images.pop();
      images.unshift(lastImage);
      updateImageOrder();
    });

    button2.addEventListener('click', function() {
      var firstImage = images.shift();
      images.push(firstImage);
      updateImageOrder();
    });

    function updateImageOrder() {
      imageContainer.innerHTML = '';
      images.forEach(function(image) {
        imageContainer.appendChild(image);
      });
    }
  </script>
</body>
